<%= sport.htmlise_title_on_show %>
<%= sport.htmlise_features_on_show %>
<%= sport.services_on_show %>
<% if sport.has_photogallery? %>
  <div class="photogallery">
    <ul>
      <% sport.images.each do |image| %>
        <li><%= link_to(image_tag_for(image.url, 124, 94), image.url, rel: :info_gallery) %></li>
      <% end %>
    </ul>
  </div>
<% end %>
